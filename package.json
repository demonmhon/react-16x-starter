{
  "name": "react-16x-starter",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0 <=12.0.0"
  },
  "description": "React app",
  "license": "MIT",
  "author": "thanawat.petchsri@gmail.com",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js",
    "build:docker": "docker build . -t react-16x-stater",
    "build:serve": "npm run build && npm run start",
    "prettier": "prettier --check \"src/**/*.(js|jsx|css|scss)\"",
    "prettier:write": "prettier --check \"src/**/*.(js|jsx|css|scss)\" --write",
    "lint": "eslint -c .eslintrc --ext .js \"src/**/*.{js,jsx}\"",
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development webpack serve --config=config/webpack.dev.js",
    "start:docker": "docker run -d -p 3000:80 react-16x-stater",
    "analyze": "webpack --config=config/webpack.prod.js --profile --json > webpack-bundle-analyzer/stats.json && webpack-bundle-analyzer webpack-bundle-analyzer/stats.json",
    "pretest": "npm run prettier",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-stage-0": "^7.8.3",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.2.6",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prettier-plugin-style-order": "^0.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
